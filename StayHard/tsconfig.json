{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "types": ["react-native-dotenv"],
    "strict": true,
    "jsx": "react-native",
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    ".env"
  ]
}
